{
  "expo": {
    "name": "OrderDrinks",
    "slug": "OrderDrinks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/coffee-cup.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/coffee-cup.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/coffee-cup.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/coffee-cup.png"
    }
  }
}
